{
  "name": "Abusive language Detection",
  "description": "Hide Offensive Comments on Instagram.com",
  "version": "2.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "./jquery.js",
      "./background.js",
      "./popup.js"
    ]
  },
    "browser_action": {
    "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "permissions": [
  
    "activeTab",
    "http://127.0.0.1:5000/comments_prediction",
    "https://www.instagram.com/"
  ]
}
